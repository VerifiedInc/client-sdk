{
  "name": "@verifiedinc/client-sdk",
  "version": "0.0.1",
  "description": "An SDK for serving Verified's 1-click service into web applications.",
  "author": "Verified",
  "main": "./dist/index.esm.js",
  "module": "./dist/index.esm.js",
  "jsnext:main": "./dist/index.esm.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
